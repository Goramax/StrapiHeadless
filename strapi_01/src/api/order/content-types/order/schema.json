{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "price": {
      "type": "decimal"
    },
    "date": {
      "type": "datetime"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "ingredients": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ingredient.ingredient"
    },
    "Reference": {
      "type": "customField",
      "options": {
        "uuid-format": "^ORDR-[A-Z]{3}-\\d{3}$"
      },
      "customField": "plugin::strapi-advanced-uuid.uuid"
    }
  }
}
